{
	"name": "domainr-node",
	"version": "1.0.1",
	"main": "dist/cjs/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"funding": "https://sponsor.marvinschopf.com/domainr-node",
	"repository": "https://github.com/marvinschopf/domainr-node",
	"keywords": [
		"domainr",
		"domains",
		"domainr api",
		"domainer",
		"domains api",
		"api"
	],
	"devDependencies": {
		"@types/node": "14.14.33",
		"@types/node-fetch": "2.5.8",
		"@types/validator": "13.1.3",
		"ava": "3.15.0",
		"prettier": "2.2.1",
		"ts-node": "9.1.1",
		"typescript": "4.2.3",
		"validator": "13.5.2"
	},
	"dependencies": {
		"node-fetch": "2.6.1"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
		"style:fix": "prettier --write **/*.ts **/*.json **/*.md .github/",
		"style:check": "prettier --check **/*.ts **/*.json **/*.md .github/",
		"test": "TS_NODE_PROJECT=tsconfig-cjs.json ava"
	},
	"files": [
		"dist/",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"prettier": {
		"useTabs": true,
		"semi": true,
		"tabWidth": 4,
		"singleQuote": false
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
